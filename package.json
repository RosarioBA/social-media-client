{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "start-server-and-test start:ci http://localhost:5500 'cypress run'",
    "test-unit": "jest",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "start:ci": "sass src/scss:dist/css && live-server --port=5500 --no-browser",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,cjs}\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@eslint/js": "8.44",
    "@testing-library/jest-dom": "^6.5.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.15.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.54.8",
    "start-server-and-test": "^2.0.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,cjs}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
